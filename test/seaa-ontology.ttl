###  http://www.seaa.ch/ontologies/2020/7/seaa-statement#decisionTime
:decisionTime rdf:type owl:DatatypeProperty ;
              rdfs:domain :statement ;
              rdfs:range xsd:dateTime ;
              rdfs:comment "The time when an event was decided to happen in the (modelled) reality" ;
              rdfs:label "decision time"@en .


###  http://www.seaa.ch/ontologies/2020/7/seaa-statement#deletionTime
:deletionTime rdf:type owl:DatatypeProperty ;
              rdfs:domain :statement ;
              rdfs:range xsd:dateTime ;
              rdfs:comment "The time when the fact is deleted in the database." ;
              rdfs:label "deletion time"@en .


###  http://www.seaa.ch/ontologies/2020/7/seaa-statement#transactionTime
:transactionTime rdf:type owl:DatatypeProperty ;
                 rdfs:domain :statement ;
                 rdfs:range xsd:dateTime ;
                 rdfs:comment "The time when the fact is stored in the database." ;
                 rdfs:label "transaction time"@en .


###  http://www.seaa.ch/ontologies/2020/7/seaa-statement#validFrom
:validFrom rdf:type owl:DatatypeProperty ;
           rdfs:domain :statement ;
           rdfs:range xsd:dateTime ;
           rdfs:comment "The starting time when the fact is true in the (modelled) reality." ;
           rdfs:label "valid from"@en .


###  http://www.seaa.ch/ontologies/2020/7/seaa-statement#validTo
:validTo rdf:type owl:DatatypeProperty ;
         rdfs:domain :statement ;
         rdfs:range xsd:dateTime ;
         rdfs:comment "The ending time when the fact is true in the (modelled) reality." ;
         rdfs:label "valid to"@en .


